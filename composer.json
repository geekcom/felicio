{
  "name": "geekcom/felicio",
  "description": "A simple lib to connect PHP with AWS SQS",
  "license": "MIT",
  "keywords": [
    "sqs",
    "queues",
    "aws"
  ],
  "authors": [
    {
      "name": "Daniel Rodrigues Lima",
      "email": "danielrodrigues-ti@hotmail.com",
      "role": "lead"
    }
  ],
